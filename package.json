{
  "name": "global-wealth-id",
  "version": "1.0.0",
  "description": "Complete full-stack Global Wealth ID credit score conversion application",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "docker:build": "docker build -t global-wealth-id .",
    "docker:run": "docker run -p 3000:3000 global-wealth-id",
    "docker:dev": "docker-compose up --build",
    "clean": "rm -rf frontend/dist backend/dist"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/global-wealth-id.git"
  },
  "author": "Global Wealth ID Team",
  "license": "MIT",
  "keywords": ["credit-score", "conversion", "full-stack", "react", "nodejs", "typescript"]
}
